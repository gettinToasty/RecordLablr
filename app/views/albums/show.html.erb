<h1><%= @album.title %></h1>
<br />

<% @album.tracks.each do |track| %>
  <%= link_to "#{track.title}", track_url(track) %><br>
<% end %>

<br><br>
<%= link_to "Add a track", new_album_track_url(@album) %>
<br>
<%= link_to "Edit this album", edit_album_url(@album) %>
<br><br>
<%= link_to "Back to band page", band_url(@album.band_id) %>
